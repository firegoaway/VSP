    <region id="0" left="0" top="0" width="53" height="24" border="true" color="#000000" bgColor="#ffffff" fontSize="10">
      <text lang="rus">
        <p>Дано:</p>
      </text>
    </region>
    <region id="1" left="90" top="0" width="78" height="41" color="#000000" bgColor="#ffffff" fontSize="10">
      <math>
        <input>
          <e type="operand">v01</e>
